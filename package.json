{
  "name": "lytx",
  "version": "0.0.01",
  "private": true,
  "type": "module",
  "scripts": {
    "initial-setup": "cp wrangler_example.jsonc wrangler.jsonc && cp example.dev.vars .dev.vars && bun run cf-types && bun run db:migrate:local",
    "dev": "vite dev",
    "build": "vite build",
    "clean": "bun run clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "db_analytics:migrate": "bunx drizzle-kit generate --config=db/postgres/drizzle.config.ts",
    "db:migrate": "drizzle-kit generate --config=db/d1/drizzle.config.ts",
    "db:migrate:local": "wrangler d1 migrations apply lytx_core_db --local",
    "db:migrate:prd": "wrangler d1 migrations apply lytx_core_db --remote",
    "db:init": "bun run cli/init-db.ts",
    "db:seed": "bun run cli/seed-data.ts",
    "db:studio": "drizzle-kit studio --config=db/d1/drizzle.config.ts",
    "cf-types": "wrangler types",
    "deploy": "wrangler publish",
    "release": "rw-scripts ensure-deploy-env && bun run clean && RWSDK_DEPLOY=1 bun run build && wrangler deploy",
    "pre-release": "rw-scripts ensure-deploy-env && RWSDK_DEPLOY=1 bun run build && wrangler deploy",
    "prepare": "vibe-rules install unified"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "@types/better-sqlite3": "^7.6.13",
    "@types/mustache": "^4.2.2",
    "@types/node": "^24.0.0",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.4",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.3.2",
    "vibe-rules": "^0.3.40",
    "wrangler": "^4.22.0"
  },
  "dependencies": {
    "@nivo/bar": "^0.99.0",
    "@nivo/core": "^0.99.0",
    "@nivo/funnel": "^0.99.0",
    "@nivo/geo": "^0.99.0",
    "@nivo/line": "^0.99.0",
    "@nivo/pie": "^0.99.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-table": "^8.21.3",
    "alchemy": "^0.45.6",
    "better-auth": "^1.2.9",
    "better-sqlite3": "^12.2.0",
    "device-detector-js": "^3.0.3",
    "drizzle-orm": "^0.44.2",
    "mustache": "^4.2.0",
    "mysql2": "^3.14.2",
    "postgres": "^3.4.3",
    "rwsdk": "^0.1.3",
    "vite": "^6.3.5"
  }
}
